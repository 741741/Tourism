.banner{
  width: 100%;
  height: 200px;
}
.banner image{
  width: 100%;
  height: 100%;
}

/* 标题 */
.list_title {
    line-height: 25px;
    padding: 8px 5px;
    border-bottom: #e7e7e5 1px solid;
}
.list_title .list_title_tit {
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    align-items: center;
    font-size: 16px;
    color: #232323;
    font-weight: bold;
}
.icon{
  font-family: travel_touch!important;
    align-self: flex-start;
    color: #fff;
    background: #4DCBFF;
    display: inline-block;
    width: 20px;
    min-width: 20px;
    height: 20px;
    border-radius: 10px;
    text-align: center;
    line-height: 20px;
    font-size: 12px;
    margin-top: 3px;
    margin-right: 6px;
    margin-left: 11px;
}
.link_comment {
    min-width: 35%;
    flex-grow: 1;
    text-align: right;
    float: none;
    font-size: 15px;
    margin-right: 0;
    font-weight: normal;
    color: #25a4bb;
}
.des_first{
  font-size: 14px;
    line-height: 22px;
    color: #333;
}

list_item .list_pack {
    padding: 14px 0;
}
.list {
    position: relative;
    height: 1%;
    line-height: 22px;
    border-bottom: #e7e7e5 1px solid;
    padding:10px 10px;
}
.item_tit {
    float: left;
    display: block;
    color: #959595;
    line-height: 22px;
    font-size: 14px;
}
.item_cout{
  display: block;
    margin: 0 0 0 45px;
    line-height: 22px;
    color: #232323;
    font-size: 14px;
}
.holiday{
   font-size: 14px;
   
}
.more_link2{
  width:20px;
  height:20px;
  float: right;
}
.icons{
  width: 10px;
  height: 10px;
}
.hide{
 display: none;
}
.show{ 
 display: block;
}
.car{
   font-size: 12px; 
}