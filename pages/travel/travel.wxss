.travel{
  width: 100%;
  height:auto;
  background:#F1F2F6;
  padding-bottom:20px
}
.bg{
  width: 100%;
  height: 174px;
  position: relative;
  margin-bottom:20px;
  text-align: center;
  
}
button.login{
  width:90px;
  height:30px;
  background: #EAA02C;
  color: #fff;
  margin-top:12px;
  line-height:30px;
  font-size: 12px;
  margin-bottom:50px;
}
.bg image{
  width: 100%;
  height: 100%;
}
.bg_portrait{
  width: 65px;
  height: 65px;
  border-radius: 50%;
  border: 1px solid #ccc;
  position: absolute;
  top:20%;
  left: 41%;
  color:#fff;
 animation:mymove 5s infinite;
}

@keyframes mymove
{
  from {left:10px;}
  to {left:200px;}
}  

.bg_portrait image{
  border-radius: 50%;
}
/* 列表开始 */
.travel_list{
  overflow: hidden;
  padding: 12px 15px;
  border-bottom: 1px solid #ECECEC;
  background: #fff;
  margin-bottom:5px;
}
.travel_list_name{
  float: left;
}
.travel_list_Symbol{
  float: right;
}
/* 列表结束 */